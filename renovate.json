{
  "extends": ["config:base"],
	"assignAutomerge": true,
	"automerge": true,
  "packageRules": [
    {
      "depTypeList": ["dependencies"],
      "updateTypes": ["patch"],
      "automerge": true
    },
    {
      "depTypeList": ["devDependencies"],
      "updateTypes": ["minor", "patch"],
      "automerge": true
    }
  ],
  "prHourlyLimit": 5,
  "prConcurrentLimit": 10,
  "bumpVersion": null,
  "rangeStrategy": "bump",
  "timezone": "Europe/Riga"
}
